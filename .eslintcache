",[],"C:\\Users\\David Setiawan\\Documents\\Website Development\\UPennXSTA_react\\upennxsta_react\\src\\Components\\Footer.jsx",["95","96"],"import React from \"react\";\r\n\r\nconst Footer = () => {\r\n    return (\r\n        <div id=\"footer\" className=\"container-fluid\">\r\n            <div className=\"container\">\r\n                <div className=\"row\">\r\n                    <div className=\"footer-section col-12 col-md-4 d-flex-column justify-content-start\">\r\n                        <h5>Quick Links</h5>\r\n                        <div><a>About us</a></div>\r\n                        <div><a>Our works</a></div>\r\n                    </div>\r\n                    <div className=\"footer-section col-12 col-md-4 d-flex-column justify-content-start\">\r\n                        <h5>Contact Us</h5>\r\n                    </div>\r\n                    <div className=\"footer-section col-12 col-md-4 d-flex-column justify-content-start\">\r\n                        <h5>Our Vision</h5>\r\n                        <p>Merry Christmas and a Happy New Year! This is filler text</p>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default Footer;","C:\\Users\\David Setiawan\\Documents\\Website Development\\UPennXSTA_react\\upennxsta_react\\src\\Components\\Router.jsx",[],"C:\\Users\\David Setiawan\\Documents\\Website Development\\UPennXSTA_react\\upennxsta_react\\src\\Components\\Routes\\AboutUs.jsx",[],"C:\\Users\\David Setiawan\\Documents\\Website Development\\UPennXSTA_react\\upennxsta_react\\src\\Components\\Features\\ResponsiveImage.jsx",[],"C:\\Users\\David Setiawan\\Documents\\Website Development\\UPennXSTA_react\\upennxsta_react\\src\\Components\\Features\\Jumbotron.jsx",[],"C:\\Users\\David Setiawan\\Documents\\Website Development\\UPennXSTA_react\\upennxsta_react\\src\\Components\\Features\\Slideshow.jsx",["97"],"import React, { useEffect, useState } from \"react\";\r\nimport ResponsiveImage from \"./ResponsiveImage\";\r\n\r\nimport { IoChevronBackCircleSharp, IoChevronForwardCircleSharp } from \"react-icons/io5\";\r\n\r\nconst Slideshow = (props) => {\r\n    const { urls, minHeight, auto, styles, classes, children } = props;\r\n\r\n    //Subcomponent\r\n    const ScrollButtons = (props) => {\r\n        const { leftButton } = props;\r\n\r\n        const setWidth = \"30%\";\r\n\r\n        return (\r\n            <div style={{ width: setWidth }} className={`slideshow-scroll-button d-flex align-items-center justify-content-${leftButton ? \"start\" : \"end\"}`}>\r\n                {leftButton ? <IoChevronBackCircleSharp onClick={leftButton ? prevImage : nextImage} size={setWidth} /> : <IoChevronForwardCircleSharp onClick={leftButton ? prevImage : nextImage} size={setWidth} />}\r\n            </div>\r\n        );\r\n    };\r\n\r\n    const interval = 6000;\r\n    let initialImages = [];\r\n\r\n    if (urls && urls.length > 0) {\r\n        for (let i = 0; i < urls.length; i++) {\r\n            initialImages[i] = {\r\n                index: i,\r\n                opacity: 0,\r\n                prev: null,\r\n                next: null\r\n            };\r\n        }\r\n\r\n        for (let i = 0; i < initialImages.length; i++) {\r\n            if (i + 1 < initialImages.length) {\r\n                initialImages[i].next = initialImages[i + 1];\r\n            } else if (i + 1 === initialImages.length) {\r\n                initialImages[i].next = initialImages[0];\r\n            }\r\n\r\n            if (i - 1 >= 0) {\r\n                initialImages[i].prev = initialImages[i - 1];\r\n            } else if (i - 1 === -1) {\r\n                initialImages[i].prev = initialImages[initialImages.length - 1];\r\n            }\r\n        }\r\n\r\n        initialImages[0].opacity = 1;\r\n    }\r\n\r\n    const [images, setImages] = useState(initialImages);\r\n    const [currImage, setCurrImage] = useState(0);\r\n\r\n    useEffect(() => {\r\n        let autoTimer = null;\r\n\r\n        if (urls && urls.length > 0 && images.length > 0) {\r\n            if (auto) {\r\n                autoTimer = setTimeout(() => {\r\n                    nextImage();\r\n                }, interval);\r\n            }\r\n        }\r\n\r\n        return (() => (auto) && (autoTimer != null) && (unmount(autoTimer)));\r\n    }, [images]);\r\n\r\n    const unmount = (timer) => {\r\n        clearTimeout(timer);\r\n    };\r\n\r\n    const prepImages = () => {\r\n        if (urls && urls.length > 0 && images.length > 0) {\r\n            let index = 0;\r\n\r\n            return urls.map(url => {\r\n                const key = index;\r\n\r\n                let imageCSS = {\r\n                    transitionDuration: auto ? \"2s\" : \"0.5s\",\r\n                    opacity: images[index].opacity\r\n                };\r\n\r\n                if (index === 0) {\r\n                    imageCSS = { ...imageCSS, position: \"relative\" };\r\n                }\r\n\r\n                index += 1;\r\n\r\n                return <ResponsiveImage key={key} url={url} minHeight={minHeight} backgroundSize=\"cover\" styles={imageCSS} classes=\"slideshow-image\" />\r\n            });\r\n        };\r\n    }\r\n\r\n    const nextImage = () => {\r\n        let newImages = [...images];\r\n\r\n        newImages[currImage].opacity = 0;\r\n        newImages[currImage].next.opacity = 1;\r\n\r\n        setCurrImage(newImages[currImage].next.index);\r\n        setImages(newImages);\r\n    };\r\n\r\n    const prevImage = () => {\r\n        let newImages = [...images];\r\n\r\n        newImages[currImage].opacity = 0;\r\n        newImages[currImage].prev.opacity = 1;\r\n\r\n        setCurrImage(newImages[currImage].prev.index);\r\n\r\n        setImages(newImages);\r\n    };\r\n\r\n    const prepScrollButtons = () => {\r\n        return (\r\n            <div style={{ position: \"absolute\", height: \"100%\", width: \"100%\" }} className=\"d-flex justify-content-between\">\r\n              