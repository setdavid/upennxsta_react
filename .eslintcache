\n// export const defaultLocation = \"/upennxsta_react\";\r\n// export const defaultLocation = \"\";\r\n\r\nconst Router = () => {\r\n    const dispatch = useDispatch();\r\n    const location = useLocation();\r\n\r\n    useEffect(() => {\r\n        dispatch(closeSlider());\r\n        scrollToTop();\r\n    }, [location]);\r\n\r\n    return (\r\n        <Switch>\r\n            {/* <Route exact path={`${defaultLocation}/`} component={Home} />\r\n            <Route exact path={`${defaultLocation}/about-us`} component={AboutUs} />\r\n            <Route exact path={`${defaultLocation}/what-weve-done`} component={WhatWeveDone} />\r\n            <Route exact path={`${defaultLocation}/recent-updates`} component={RecentUpdates} /> */}\r\n            <Route exact path={`/`} component={Home} />\r\n            <Route exact path={`/about-us`} component={AboutUs} />\r\n            <Route exact path={`/what-weve-done`} component={WhatWeveDone} />\r\n            <Route exact path={`/recent-updates`} component={RecentUpdates} />\r\n        </Switch>\r\n    );\r\n}\r\n\r\nexport default Router;","C:\\Users\\David Setiawan\\Documents\\Website Development\\UPennXSTA_react\\upennxsta_react\\src\\Components\\Routes\\AboutUs.jsx",[],"C:\\Users\\David Setiawan\\Documents\\Website Development\\UPennXSTA_react\\upennxsta_react\\src\\Components\\Features\\ResponsiveImage.jsx",[],"C:\\Users\\David Setiawan\\Documents\\Website Development\\UPennXSTA_react\\upennxsta_react\\src\\Components\\Features\\Jumbotron.jsx",[],"C:\\Users\\David Setiawan\\Documents\\Website Development\\UPennXSTA_react\\upennxsta_react\\src\\Components\\Features\\Slideshow.jsx",["130"],"import React, { useEffect, useState, useRef } from \"react\";\r\nimport ResponsiveImage from \"./ResponsiveImage\";\r\nimport { CSSTransition, TransitionGroup } from \"react-transition-group\";\r\n\r\nimport { IoChevronBackCircleSharp, IoChevronForwardCircleSharp } from \"react-icons/io5\";\r\n\r\nconst Slideshow = (props) => {\r\n    const { urls, minHeight, auto, styles, classes, children } = props;\r\n\r\n    //Subcomponent\r\n    const ScrollButtons = (props) => {\r\n        const { leftButton } = props;\r\n\r\n        const setWidth = \"30%\";\r\n\r\n        return (\r\n            <div style={{ width: setWidth }} className={`slideshow-scroll-button d-flex align-items-center justify-content-${leftButton ? \"start\" : \"end\"}`}>\r\n                {leftButton ? <IoChevronBackCircleSharp onClick={leftButton ? prevImage : nextImage} size={setWidth} /> : <IoChevronForwardCircleSharp onClick={leftButton ? prevImage : nextImage} size={setWidth} />}\r\n            </div>\r\n        );\r\n    };\r\n\r\n    const interval = 6000;\r\n    const transitionLength = 500;\r\n    const autoTransitionLength = 2000;\r\n    let initialization = [];\r\n\r\n    if (urls && urls.length > 0) {\r\n        for (let i = 0; i < urls.length; i++) {\r\n            initialization[i] = {\r\n                id: i,\r\n                url: urls[i],\r\n                prev: null,\r\n                next: null\r\n            };\r\n        }\r\n\r\n        for (let i = 0; i < initialization.length; i++) {\r\n            if (i + 1 < initialization.length) {\r\n                initialization[i].next = initialization[i + 1];\r\n            } else if (i + 1 === initialization.length) {\r\n                initialization[i].next = initialization[0];\r\n            }\r\n\r\n            if (i - 1 >= 0) {\r\n                initialization[i].prev = initialization[i - 1];\r\n            } else if (i - 1 === -1) {\r\n                initialization[i].prev = initialization[initialization.length - 1];\r\n            }\r\n        }\r\n    }\r\n\r\n    const images = useRef(initialization);\r\n    const [currImage, setCurrImage] = useState(0);\r\n\r\n    useEffect(() => {\r\n        let autoTimer = null;\r\n\r\n        if (urls && urls.length > 0 && images.current.length > 0) {\r\n            if (auto) {\r\n                autoTimer = setTimeout(() => {\r\n                    console.log(\"next\");\r\n                    nextImage();\r\n                }, interval);\r\n            }\r\n        }\r\n\r\n        return (() => (auto) && (autoTimer != null) && (unmount(autoTimer)));\r\n    }, [currImage]);\r\n\r\n    const unmount = (timer) => {\r\n        console.log(\"auto slideshow image unmount\");\r\n        clearTimeout(timer);\r\n    };\r\n\r\n    const prepImage = () => {\r\n        if (urls && urls.length > 0 && images.current.length > 0) {\r\n            return <ResponsiveImage styles={{ transitionDuration: `${auto ? autoTransitionLength : transitionLength}ms` }} url={images.current[currImage].url} minHeight={minHeight} backgroundSize=\"cover\" classes=\"slideshow-image\" />\r\n        };\r\n    };\r\n\r\n    const nextImage = () => {\r\n        console.log(\"next image\");\r\n        setCurrImage(images.current[currImage].next.id);\r\n    };\r\n\r\n    const prevImage = () => {\r\n        setCurrImage(images.current[currImage].prev.id);\r\n    };\r\n\r\n    const prepScrollButtons = () => {\r\n        return (\r\n            <div style={{ position: \"absolute\", height: \"100%\", width: \"100%\" }} className=\"d-flex justify-content-between\">\r\n                <ScrollButtons leftButton />\r\n                <ScrollButtons />\r\n            </div>\r\n        );\r\n    };\r\n\r\n    return (\r\n        <div style={{ ...styles, minHeight: minHeight }} className={`${classes ? classes : \"\"} slideshow d-flex align-items-center`} >\r\n            <TransitionGroup>\r\n                <CSSTransition key={images.current[currImage].id} timeout={auto ? autoTransitionLength : transitionLength} classNames=\"fade\">\r\n                    {prepImage()}\r\n                </CSSTransition>\r\n            </TransitionGroup>\r\n            {!auto && prepScrollButtons()}\r\n            {children}\r\n        </div >\r\n    );\r\n}\r\n\r\n\r\nexport default Slideshow;","C:\\Users\\David Setiawan\\Documents\\Website De