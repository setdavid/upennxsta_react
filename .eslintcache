    initialization[i] = {\r\n                id: i,\r\n                url: urls[i],\r\n                prev: null,\r\n                next: null\r\n            };\r\n        }\r\n\r\n        for (let i = 0; i < initialization.length; i++) {\r\n            if (i + 1 < initialization.length) {\r\n                initialization[i].next = initialization[i + 1];\r\n            } else if (i + 1 === initialization.length) {\r\n                initialization[i].next = initialization[0];\r\n            }\r\n\r\n            if (i - 1 >= 0) {\r\n                initialization[i].prev = initialization[i - 1];\r\n            } else if (i - 1 === -1) {\r\n                initialization[i].prev = initialization[initialization.length - 1];\r\n            }\r\n        }\r\n    }\r\n\r\n    const images = useRef(initialization);\r\n    const [currImage, setCurrImage] = useState(0);\r\n\r\n    useEffect(() => {\r\n        let autoTimer = null;\r\n\r\n        if (urls && urls.length > 0 && images.current.length > 0) {\r\n            if (auto) {\r\n                autoTimer = setTimeout(() => {\r\n                    console.log(\"next\");\r\n                    nextImage();\r\n                }, interval);\r\n            }\r\n        }\r\n\r\n        return (() => (auto) && (autoTimer != null) && (unmount(autoTimer)));\r\n    }, [currImage]);\r\n\r\n    const unmount = (timer) => {\r\n        console.log(\"auto slideshow image unmount\");\r\n        clearTimeout(timer);\r\n    };\r\n\r\n    const prepImage = () => {\r\n        if (urls && urls.length > 0 && images.current.length > 0) {\r\n            return <ResponsiveImage styles={{ transitionDuration: `${auto ? autoTransitionLength : transitionLength}ms` }} url={images.current[currImage].url} minHeight={minHeight} backgroundSize=\"cover\" classes=\"slideshow-image\" />\r\n        };\r\n    };\r\n\r\n    const nextImage = () => {\r\n        console.log(\"next image\");\r\n        setCurrImage(images.current[currImage].next.id);\r\n    };\r\n\r\n    const prevImage = () => {\r\n        setCurrImage(images.current[currImage].prev.id);\r\n    };\r\n\r\n    const prepScrollButtons = () => {\r\n        return (\r\n            <div style={{ position: \"absolute\", height: \"100%\", width: \"100%\" }} className=\"d-flex justify-content-between\">\r\n                <ScrollButtons leftButton />\r\n                <ScrollButtons />\r\n            </div>\r\n        );\r\n    };\r\n\r\n    return (\r\n        <div style={{ ...styles, minHeight: minHeight }} className={`${classes ? classes : \"\"} slideshow d-flex align-items-center`} >\r\n            <TransitionGroup>\r\n                <CSSTransition key={images.current[currImage].id} timeout={auto ? autoTransitionLength : transitionLength} classNames=\"fade\">\r\n                    {prepImage()}\r\n                </CSSTransition>\r\n            </TransitionGroup>\r\n            {!auto && prepScrollButtons()}\r\n            {children}\r\n        </div >\r\n    );\r\n}\r\n\r\n\r\nexport default Slideshow;","C:\\Users\\David Setiawan\\Documents\\Website Development\\UPennXSTA_react\\upennxsta_react\\src\\Components\\Features\\TextOverImage.jsx",[],"C:\\Users\\David Setiawan\\Documents\\Website Development\\UPennXSTA_react\\upennxsta_react\\src\\Components\\Features\\FadePassNavbar.jsx",[],"C:\\Users\\David Setiawan\\Documents\\Website Development\\UPennXSTA_react\\upennxsta_react\\src\\Components\\Features\\ColoredCard.jsx",[],"C:\\Users\\David Setiawan\\Documents\\Website Development\\UPennXSTA_react\\upennxsta_react\\src\\Components\\Features\\SetNavbarTrans.jsx",["152"],"import React, { useEffect, useRef } from \"react\";\r\nimport { useDispatch } from \"react-redux\";\r\nimport { transparentizeNavbar, fillNavbar } from \"./../../redux/ducks/navbar\";\r\nimport { NAVBAR_HEIGHT } from \"../Navbar\";\r\n\r\nconst SetNavbarTrans = (props) => {\r\n    const { styles, classes, children } = props;\r\n\r\n    const elemRef = useRef();\r\n    const dispatch =